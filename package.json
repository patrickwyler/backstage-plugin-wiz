{
  "name": "backstage-plugin-wiz",
  "version": "1.0.0",
  "private": false,
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev": "dotenv -- pnpm -r start",
    "start": "pnpm -r --filter @wiz-sec/backstage-plugin-wiz start",
    "start:backend": "pnpm -r --filter @wiz-sec/backstage-plugin-wiz-backend start",
    "build": "backstage-cli repo build",
    "clean": "backstage-cli repo clean",
    "test": "jest",
    "lint": "eslint --cache",
    "tsc": "tsc --build tsconfig.json",
    "tsc:clean": "tsc --build --clean"
  },
  "workspaces": {
    "packages": [
      "plugins/*"
    ]
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@backstage/cli": "^0.29.4",
    "@eslint/js": "^9.17.0",
    "@spotify/prettier-config": "^15.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "concurrently": "^9.1.2",
    "cross-fetch": "^4.1.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.19.0"
  },
  "prettier": "@spotify/prettier-config",
  "packageManager": "pnpm@9.15.2"
}
